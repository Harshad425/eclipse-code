		System.out.println("program is starte");
		
		try
		{
		String s=null;
		System.out.println(s.length());
		}
		catch ( java.lang.NullPointerException e)
		{
			
		}
		System.out.println("program is completed");
		System.out.println("program is exited");
		
	}

}
